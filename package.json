{
  "name": "redux-jumpstart",
  "description": "Minimalistic boilerplate code for a react/redux application, with react-router and webpack to jumpstart your project.",
  "version": "0.0.1",
  "main": "index.js",
  "scripts": {
    "jumpstart": "npm install && npm run dev",
    "dev": "node src/server/dev/build-message-dev && node src/server/dev/devServer",
    "dev-https": "node src/server/dev/build-message-dev && node src/server/dev/devServer https=true",
    "production": "node src/server/production/build-message && npm run create-production && node src/server/production/productionServer",
    "create-production": "NODE_ENV=production webpack -p --config webpack.production.config.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Rory Kermack",
  "license": "WTFPL",
  "dependencies": {
    "classnames": "^2.2.5",
    "css-loader": "^0.23.1",
    "express": "^4.14.0",
    "firebase": "^3.6.5",
    "immutable": "^3.8.1",
    "node-sass": "^3.8.0",
    "react": "^15.2.0",
    "react-addons-shallow-compare": "^15.2.1",
    "react-dom": "^15.2.0",
    "react-pure-render": "^1.0.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.2",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-simple-router": "^2.0.4",
    "redux-storage": "^4.0.1",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel-runtime": "^5.8.20",
    "react-transform-hmr": "^1.0.4",
    "babel-core": "^5.8.3",
    "babel-loader": "^5.3.2",
    "babel-plugin-react-transform": "^1.1.0",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.16.0",
    "mocha": "^3.0.2",
    "node-sass": "^3.8.0",
    "sass-loader": "^4.0.0",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.12.0",
    "redux-logger": "^2.6.1",
    "style-loader": "^0.13.1"
  }
}
